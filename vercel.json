{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_API_URL": "https://backend-2-71va.onrender.com",
    "BACKEND_URL": "https://backend-2-71va.onrender.com"
  },
  
  "regions": ["iad1"],
  "framework": "nextjs"
}
